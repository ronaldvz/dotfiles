#ronald's bash profile 

#ls aliases
alias ls='ls -lG'
alias ll='ls -l'
alias lsa='ls -all'

#git aliases
alias g='git'
alias gs='git status -s'
alias gc='git commit -m'
alias gd='git diff'
alias ga='git add'
alias gp='git push'

#dirs
alias ~='cd ~'
alias dev='cd ~/development'

if [ -f ~/.bashrc ]; then 
	source ~/.bashrc
fi
